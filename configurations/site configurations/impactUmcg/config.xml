            class="org.rsna.ctp.stdstages.DicomAnonymizer"
			root="/srv/data/ctpData/IMPACT_RCC/UMCG/roots/dicom-anonymizer"
			lookupTable="/srv/data/sftp/impactumcg/lookup/renal_lookup.properties"
            script="ctpConfig/IMPACT_RCC/UMCG/anonymizer.properties"
			quarantine="/srv/data/sftp/impactumcg/anonymize-failed/RCC/" />
		<Processor
            name="PatientIdFilter"
            class="org.rsna.ctp.stdstages.DicomFilter"
			root="/srv/data/ctpData/IMPACT_RCC/UMCG/roots/dicom-filter"
            script="ctpConfig/IMPACT_RCC/UMCG/filter.script"
			quarantine="/srv/data/ctpData/IMPACT_RCC/UMCG/quarantines/dicom-filter" />
    	<ExportService

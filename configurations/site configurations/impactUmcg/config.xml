<Configuration>

   <Server port="8080" />

   <Pipeline name="IMPACT_UMCG">
		<ImportService
			name="DicomImportService"
			id="DicomImportService"
            class="org.rsna.ctp.stdstages.DicomImportService"
			root="ctpData/IMPACT/UMCG/roots/dicomImportService"
			port="104"
			
            logConnections="yes"
            logDuplicates="no"
            suppressDuplicates="no">
			
			<!-- <accept calledAET="" />
			<accept ip="" />
			<accept callingAET="" />
			<accept calledAET="" /> -->
						
		</ImportService>
        <ImportService
            name="Client Directory Import"
            class="org.rsna.ctp.stdstages.DirectoryImportService"
            root="ctpData/IMPACT/UMCG/roots/directory-import"
            minAge="60000"
            quarantine="ctpData/IMPACT/UMCG/quarantines/directory-import" />
        <DicomAnonymizer
            name="Client DICOM Anonymizer"
            class="org.rsna.ctp.stdstages.DicomAnonymizer"
            root="ctpData/IMPACT/UMCG/roots/dicom-anonymizer"
            lookupTable="ctpConfig/IMPACT/UMCG/lookup.properties"
            script="ctpConfig/IMPACT/UMCG/anonymizer.properties"
            quarantine="ctpData/IMPACT/UMCG/quarantines/dicom-anonymizer" />
		<Processor
            name="PatientIdFilter"
            class="org.rsna.ctp.stdstages.DicomFilter"
            root="ctpData/IMPACT/UMCG/roots/dicom-filter"
            script="ctpConfig/IMPACT/UMCG/filter.script"
            quarantine="ctpData/IMPACT/UMCG/quarantines/dicom-filter" />
        <!-- <ExportService
            name="HTTP Export Service"
            class="org.rsna.ctp.stdstages.HttpExportService"
            root="ctpData/IMPACT/UMCG/roots/export-service" 
            url="https://ctp.bmia.nl:443"
            acceptDicomObjects="yes"
            acceptXmlObjects="yes"
            acceptZipObjects="yes"
            acceptFileObjects="yes"
            interval="5000" /> -->
		<DirectoryStorageService
            name="DirectoryStorageService"
            id="stage ID"
            structure="(0008,0080)/(0010,0020)"
            defaultString="UNKNOWN"
            whitespaceReplacement="_"
            class="org.rsna.ctp.stdstages.DirectoryStorageService"
            root="ctpData/IMPACT/UMCG/roots/storage"
            quarantine="ctpData/IMPACT/UMCG/quarantines/exportService" />
   </Pipeline>

</Configuration>